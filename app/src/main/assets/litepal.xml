<?xml version="1.0" encoding="utf-8"?>
<litepal>
    <!--数据库名称-->
    <dbname value="weather"/>
    <!--数据库版本号-->
    <version value="4" />
    <!--用于设定所有的映射模型,即你定义数据库表的类名路径-->
    <list>
        <mapping class="com.example.weather02.db.Province" />
        <mapping class="com.example.weather02.db.City" />
        <mapping class="com.example.weather02.db.County" />
    </list>
</litepal>